{
  "name": "listthisbackend",
  "version": "1.0.0",
  "description": "Backend for react native app",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "start:dev": "nodemon server/ --ignore tests/",
    "userModelTest": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/modelsTest/user.spec.js --watch --runInBand --verbose",
    "listModelTest": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/modelsTest/list.spec.js --watch --runInBand --verbose",
    "taskModelTest": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/modelsTest/task.spec.js --watch --runInBand --verbose",
    "test": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/*/*.spec.js --watch --runInBand --verbose",
    "test:dev": "ENV=TEST npm run test",
    "test:dev:model": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/modelsTest/*.spec.js --watch --runInBand --verbose",
    "test:dev:api": "DATABASE_URL=postgres://localhost/listthisdbtest jest ./tests/apiTest/*.spec.js --watch --runInBand --verbose"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jwt-simple": "^0.5.6",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.2.3",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
